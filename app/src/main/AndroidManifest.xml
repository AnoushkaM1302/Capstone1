<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.takeabreak.breaktab">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BreakTab">
        <activity android:name=".Info"></activity>
        <activity android:name=".Settings" />
        <activity android:name=".Homescreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Meditation" />
        <activity android:name=".Exercise" />
        <activity android:name=".Music" />
        <activity android:name=".Yoga" />
        <activity android:name=".Tab" />
        <activity android:name=".BufferafterTimer"/>
    </application>

</manifest>